<h2> Your laundry Dashboard </h2>

<ul>
  <li> <a href="/laundry/launderers"> Find a Launderer </a> </li>
  <li> <a href="/auth/logout"> Log Out </a> </li>
</ul>


<h3> Want to become a launderer? </h3>

<form action="/laundry/launderers" method="post">
  <div>
    <label for="fee-input"> Set your fee </label>
    <input type="number" name="fee" id="fee-input">
  </div>

  <button type="submit"> Become a Launderer </button>
</form>

<h3> Pending Pickups </h3>

<ul>
  <% pickups.forEach((onePickup) => { %>
    <li>
      <h4> <%= onePickup.pickupDate %> </h4>

      <ul>
        <li> <b>User</b>: <%= onePickup.user.name %> </li>
        <li> <b>Launderer</b>: <%= onePickup.launderer.name %> </li>
      </ul>
    </li>
  <% }) %>
</ul>
